{
  "name": "example-webpack-build",
  "version": "1.0.0",
  "description": "Tutorial for Webpack",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist && ./node_modules/.bin/webpack -p",
    "start": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "deps:check": "./node_modules/.bin/npm-check-updates && ./node_modules/.bin/nsp check",
    "deps:update": "./node_modules/.bin/npm-check-updates -u && npm install && ./node_modules/.bin/nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenofdoom/js-build-pipelines-training.git"
  },
  "keywords": [
    "training",
    "webpack",
    "introduction"
  ],
  "author": "Jen Zajac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jenofdoom/js-build-pipelines-training/issues"
  },
  "homepage": "https://github.com/jenofdoom/js-build-pipelines-training#readme",
  "private": true,
  "devDependencies": {
    "autoprefixer": "~6.7.7",
    "babel-core": "~6.24.0",
    "babel-loader": "~6.4.1",
    "babel-polyfill": "~6.23.0",
    "babel-preset-env": "~1.4.0",
    "babel-preset-react": "~6.24.1",
    "bootstrap": "~4.0.0-alpha.6",
    "clipboard": "~1.6.1",
    "css-loader": "~0.28.0",
    "eslint": "~3.19.0",
    "eslint-loader": "~1.7.0",
