    "file-loader": "~0.11.1",
    "html-webpack-plugin": "~2.28.0",
    "node-sass": "~4.5.2",
    "npm-check-updates": "~2.10.5",
    "nsp": "~2.6.3",
    "postcss-flexbugs-fixes": "~2.1.0",
    "postcss-loader": "~1.3.3",
    "react": "~15.5.4",
    "react-dom": "~15.5.4",
    "react-router-dom": "~4.1.1",
    "react-scroll-up": "~1.2.1",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.16.0",
    "url-loader": "~0.5.8",
    "webpack": "~2.4.1",
